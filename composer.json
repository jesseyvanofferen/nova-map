{
    "name": "hostit-online/nova-map",
    "description": "Map field for Laravel Nova",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.1"
    },
    "autoload": {
        "psr-4": {
            "HostitOnlile\\Map\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HostitOnlile\\Map\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
